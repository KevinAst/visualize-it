{
  "name": "visualize-it",
  "version": "0.1.1",
  "description": "Your View into External Systems",
  "homepage": "https://visualize-it.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinAst/visualize-it.git"
  },
  "keywords": [
    "visualize-it",
    "svelte",
    "web-app",
    "web",
    "geeku",
    "astx"
  ],
  "license": "MIT",
  "scripts": {
    "app:prodBuild": "rollup -c",
    "app:devServe": "rollup -c -w",
    "preapp:deploy": "npm run app:prodBuild",
    "app:deploy": "gh-pages --dist public --dest app",
    "l8tr:app:lint": "eslint src",
    "l8tr:docs:serve": "gitbook serve",
    "l8tr:docs:build": "gitbook build",
    "l8tr:docs:build:COMMENT": "NOTE: for gitbook build/serve, following diagnostics are useful: --log=debug --debug",
    "l8tr:docs:prepare:do:once": "gitbook install",
    "docs:publish": "gh-pages --dist _docs --dest docs",
    "l8tr:predocs:publish": "npm run docs:build",
    "start": "sirv public",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "babel-jest": "^25.4.0",
    "crc": "^3.8.0",
    "enumify": "^2.0.0",
    "gh-pages": "^3.1.0",
    "jest": "^25.4.0",
    "konva": "^6.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^2.8.2",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sass": "^1.26.3",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.0.0",
    "svelte-material-ui": "^1.0.0-beta.21"
  }
}
